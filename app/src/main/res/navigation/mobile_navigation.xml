<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">



    <fragment
        android:id="@+id/navigation_debug"
        android:name="org.app4shm.demo.ui.debug.DebugFragment"
        android:label="@string/title_groups"
        tools:layout="@layout/fragment_debug" />

    <fragment
        android:id="@+id/navigation_welch"
        android:name="org.app4shm.demo.ui.welch.WelchFragment"
        android:label="@string/title_groups"
        tools:layout="@layout/fragment_welch" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="org.app4shm.demo.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />


    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="org.app4shm.demo.ui.risk_analysis.RiskAnalysisFragment"
        android:label="@string/title_risk_analysis"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="org.app4shm.demo.ui.damage_detection.DamageDetectionFragment"
        android:label="@string/title_damage_detection"
        tools:layout="@layout/fragment_notifications" />
</navigation>